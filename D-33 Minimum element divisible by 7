#include<stdio.h>
int main(){
    int n,ele,a[100],i,count=0;
    printf("Enter the size of array: ");
    scanf("%d",&n);
    printf("Enter the minimum elements that are divisible by 7: ");
    scanf("%d",&ele);
    printf("Enter the array elements: ");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++){
    if(a[i]%7==0)
    count++;
    
    }
    if(ele==count)
    printf("yes");
    else{
        printf("No");
    }
    return 0;
}
